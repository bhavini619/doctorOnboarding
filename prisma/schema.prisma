generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model InterestedDoctor {
  id          Int      @id @default(autoincrement())
  name        String?
  email       String?
  contactno   String?
  country     String?
  city        String?
  state       String?
  postalcode  String?
  createdAt   DateTime @default(now())
}
